page {
  background-color: #fbf9fe;
  height: 100%;
  font-size: 18px;
}

.itemView{
  margin-top: 10px;
  margin-left: 10px;
  margin-right: 10px;
  height: 50px;
  
  justify-content: flex-start;
  display: flex;
  align-items: center;
}
.label{
  width: 75px;
  margin-bottom: 8px;
}
.input{
  flex:1;
  align-self: right;
  width: 230px;
  height: 40px; 
  border: 1px solid lightgray;
  border-radius: 15px;
  padding-left: 10px;
}
.viewName{
  height: 40px;
  font-size: 25px;
  margin-top: 40px;

  justify-content: center;
  display: flex;
  align-items: center;
}
.button{
  margin-left:10px;
  margin-right:10px;
  flex:1;
}
.result{
  margin-top: 20px;
  margin-left: 10px;
  margin-right: 10px;
  background-color: peachpuff;
}